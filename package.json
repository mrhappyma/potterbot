{
  "name": "potterbot",
  "scripts": {
    "start": "echo \"Starting...\" && node built/index.js",
    "prestart": "echo \"Building...\" && npx tsc --build && echo 'Prestart completed, start the bot with `pnpm start`'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@discordjs/rest": "^1.3.0",
    "axios": "^1.6.0",
    "discord-api-types": "^0.37.16",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "fuse.js": "^6.6.2",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3"
  },
  "lint-staged": {
    "**/*": "pretty-quick --staged"
  }
}
